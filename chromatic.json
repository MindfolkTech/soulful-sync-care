  "externals": ["public/**"],
  "delay": 300,
  "diffThreshold": 0.2,
  "threshold": 0.2,
  "autoAcceptChanges": false,
  "uploadTimeout": 120000,
  "storybookBuildDir": "storybook-static"
}
